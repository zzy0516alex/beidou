<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false" >
        <shape>
            <solid
                android:color="#35B815"/>

            <corners
                android:radius="15dp"/>
        </shape>
    </item>

    <item android:state_pressed="true" >
        <shape>
            <solid
                android:color="#2C9911"/>

            <corners
                android:radius="15dp"/>
        </shape>
    </item>

</selector>